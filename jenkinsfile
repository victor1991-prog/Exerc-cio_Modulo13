pipeline {
    agent any

    stages {
        stage('Setup') {
            steps {
                git branch: 'main', url: 'https://github.com/victor1991-prog/Exercicio_Modulo13.gitt'
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                sh '''set NO_COLOR=1 npm test'''
            }
        }
    }
}